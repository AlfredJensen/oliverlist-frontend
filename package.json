{
  "name": "sales",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --port 9000 --mode local",
    "serve-dev": "vue-cli-service serve --mode development",
    "build": "vue-cli-service build",
    "sass": "node-sass src/assets/css/styles.scss src/assets/css/styles.css --output-style compressed",
    "sass2": "node-sass src/assets/css/sassComponents.scss src/assets/css/sassComponents.css --output-style compressed",
    "sass3": "node-sass src/assets/sass/bootstrap/bootstrap.scss src/assets/css/bootstrap.min.css --output-style compressed",
    "pepe1": "node-sass -w node_modules/bootstrap/scss/ -o node_modules/bootstrap/dist/css/",
    "pepe2": "node-sass --include-path scss node_modules/bootstrap/scss/ -o node_modules/bootstrap/dist/css/",
    "build-loc": "env-cmd -f .env.local vue-cli-service build",
    "build-dev": "env-cmd -f .env.development vue-cli-service build",
    "build-pro": "vue-cli-service build --mode production"
  },
  "dependencies": {
